[Unit]
Description=Dgfip Contractualization back
After=network-online.target

[Service]
Type=simple

User={{ app_user }}
Group={{ app_user }}

EnvironmentFile=/etc/dgfip-contractualization-back.conf
ExecStart=/opt/apps/dgfip-contractualization-back/dgfip-contractualization-back/bin/www

Restart=on-failure
TimeoutStopSec=30

[Install]
WantedBy=multi-user.target
