[Unit]
Description=Dgfip Contractualization oauth
After=network-online.target

[Service]
Type=simple

User={{ app_user }}
Group={{ app_user }}

EnvironmentFile=/etc/dgfip-contractualization-oauth.conf
ExecStart=/opt/apps/dgfip-contractualization-oauth/dgfip-contractualization-oauth/bin/www

Restart=on-failure
TimeoutStopSec=30

[Install]
WantedBy=multi-user.target
