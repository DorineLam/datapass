[Unit]
Description=Dgfip Contractualization back
After=network-online.target

[Service]
Type=simple

User={{ app_user }}
Group={{ app_user }}

EnvironmentFile=/etc/api-particulier-courtier-back.conf
ExecStart=/opt/apps/api-particulier-courtier-back/api-particulier-courtier-back/bin/www

Restart=on-failure
TimeoutStopSec=30

[Install]
WantedBy=multi-user.target
